    "@types/turndown": "^5.0.6",
    "@types/uuid": "^9.0.8",
    "@types/web-push": "^3.6.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "pino-pretty": "^13.1.2",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.11",
    "tsx": "^4.20.6",
    "typescript": "^5",
    "vite-tsconfig-paths": "^6.0.5",
    "vitest": "^4.0.18"
  },
  "overrides": {
