{
  "name": "amanoba",
  "version": "2.9.2",
  "private": true,
  "description": "Unified 30-day learning platform with gamified education, email-based lesson delivery, and interactive assessments — built for engagement, retention, and student success",
  "author": "Narimato",
  "engines": {
    "node": ">=20.0.0 <25.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "release:patch": "node scripts/versioning/bump-version.mjs --type patch --reason \"Patch release\" && git add -A && git commit -m \"release: v$(node scripts/versioning/get-version.mjs) — Patch release\"",
    "release:minor": "node scripts/versioning/bump-version.mjs --type minor --reason \"Minor release\" && git add -A && git commit -m \"release: v$(node scripts/versioning/get-version.mjs) — Minor release\"",
    "release:major": "node scripts/versioning/bump-version.mjs --type major --reason \"Major release\" && git add -A && git commit -m \"release: v$(node scripts/versioning/get-version.mjs) — Major release\"",
    "seed": "tsx scripts/seed-all.ts",
    "seed:core": "tsx scripts/seed-core-data.ts",
    "seed:achievements": "tsx scripts/seed-achievements.ts",
    "seed:achievements-v2": "tsx --env-file=.env.local scripts/seed-achievements-v2.ts",
    "seed:rewards": "tsx scripts/seed-rewards.ts",
    "repair:stats": "tsx scripts/repair-stats.ts",
    "workers": "tsx --env-file=.env.local scripts/start-workers.ts",
    "seed:guest-usernames": "tsx --env-file=.env.local scripts/seed-guest-usernames.ts",
    "seed:quiz-questions": "tsx scripts/seed-quiz-questions.ts",
    "seed:whackpop-emojis": "tsx scripts/seed-whackpop-emojis.ts",
    "seed:translations": "tsx scripts/seed-translations-direct.ts",
    "seed:ai-course": "tsx --env-file=.env.local scripts/seed-ai-30-nap-course.ts",
    "seed:course-creation": "tsx --env-file=.env.local scripts/seed-course-creation-course.ts",
    "seed:geo-shopify-course": "tsx --env-file=.env.local scripts/seed-geo-shopify-course.ts",
    "seed:geo-shopify-course-en": "tsx --env-file=.env.local scripts/seed-geo-shopify-course-en.ts",
    "seed:ai-course-en": "tsx --env-file=.env.local scripts/seed-ai-course-en.ts",
    "seed:survey": "tsx --env-file=.env.local scripts/seed-onboarding-survey.ts",
    "seed:productivity-lesson-8": "tsx --env-file=.env.local scripts/seed-productivity-lesson-8.ts",
    "seed:scrummaster-leszek-2026-hu": "tsx --env-file=.env.local scripts/seed-scrummaster-leszek-2026-hu.ts",
    "seed:sport-sales-network-europe-2026-en": "tsx --env-file=.env.local scripts/seed-sport-sales-network-europe-2026-en.ts",
    "seed:sport-sales-network-usa-2026-en": "tsx --env-file=.env.local scripts/seed-sport-sales-network-usa-2026-en.ts",
    "admin:set-role": "tsx --env-file=.env.local scripts/set-admin-role.ts",
    "admin:set-sso-role": "tsx --env-file=.env.local scripts/set-sso-admin-role.ts",
    "admin:restore-role": "tsx --env-file=.env.local scripts/restore-admin-role.ts",
    "admin:check-role": "tsx --env-file=.env.local scripts/check-user-role.ts",
    "migrate:fix-facebookid-index": "tsx --env-file=.env.local scripts/fix-facebookid-index.ts",
    "migrate:remove-facebookid": "tsx --env-file=.env.local scripts/migrate-remove-facebookid.ts",
    "admin:disable-course-discussion": "tsx --env-file=.env.local scripts/disable-course-discussion.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:payment-contract": "tsx scripts/payment-e2e-contract-test.ts",
    "docs:inventory": "node --import tsx scripts/docs/generate-docs-inventory.ts",
    "docs:canonical-map": "node --import tsx scripts/docs/generate-docs-canonical-map.ts",
    "docs:triage": "node --import tsx scripts/docs/generate-docs-triage.ts",
    "docs:refresh": "npm run docs:inventory && npm run docs:canonical-map && npm run docs:triage",
    "docs:check": "npm run docs:refresh && node scripts/docs/check-generated-docs.mjs",
    "ui:audit:layout": "node --import tsx scripts/audit-layout-grammar-ui.ts --write",
    "ui:check:layout": "node --import tsx scripts/audit-layout-grammar-ui.ts --check"
  },
  "dependencies": {
    "@auth/mongodb-adapter": "^3.11.0",
    "@hookform/resolvers": "^3.9.0",
    "@next/env": "^15.5.0",
