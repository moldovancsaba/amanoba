    "@tanstack/react-query": "^5.56.2",
    "@tiptap/extension-link": "^3.15.3",
    "@tiptap/extension-placeholder": "^3.15.3",
    "@tiptap/react": "^3.15.3",
    "@tiptap/starter-kit": "^3.15.3",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "axios": "^1.7.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "form-data": "^4.0.0",
    "framer-motion": "^10.18.0",
    "glob": "^13.0.0",
    "jose": "^6.1.3",
    "jszip": "^3.10.1",
    "lucide-react": "^0.462.0",
    "mailgun.js": "^12.0.0",
    "marked": "^12.0.2",
    "mongodb": "^6.18.0",
    "mongoose": "^8.18.0",
    "next": "^15.5.11",
    "next-auth": "^5.0.0-beta.29",
    "next-intl": "^4.7.0",
    "next-themes": "^0.4.6",
    "nodemailer": "^6.9.16",
    "pino": "^9.13.0",
    "qrcode": "^1.5.4",
    "rate-limiter-flexible": "^8.0.1",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.53.0",
    "react-icons": "^5.5.0",
    "react-is": "^18.3.1",
    "recharts": "^3.2.1",
    "resend": "^6.7.0",
    "server-only": "^0.0.1",
    "sonner": "^1.5.0",
    "source-map-js": "1.2.1",
    "stripe": "^20.2.0",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "to-regex-range": "5.0.1",
    "turndown": "^7.2.2",
    "uuid": "^9.0.1",
    "vaul": "^0.9.3",
    "web-push": "^3.6.7",
